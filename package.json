{
  "name": "imx-core-sdk-examples",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "test": "jest -c jest.config.js",
    "restricted-publish": "npm publish --access restricted",
    "build": "npx tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "transfer-eth": "ts-node -r dotenv/config libs/transferEth.ts",
    "transfer-erc20": "ts-node -r dotenv/config libs/transferErc20.ts"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/wallet": "^5.7.0",
    "@imtbl/core-sdk": "^1.0.0-beta.3",
    "dotenv": "^16.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@starkware-industries/starkware-crypto-utils": "^0.0.2",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
